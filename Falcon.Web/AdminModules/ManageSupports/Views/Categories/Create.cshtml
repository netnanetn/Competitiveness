@model CategoryModel

@using (Html.BeginForm("Create", "Categories", FormMethod.Post, new { enctype = "multipart/form-data" }))
{
    @Html.Partial("_Toolbar")

    @Html.ValidationSummary(true)
    <div class="entry-edit">
        <div class="entry-edit-head">
            <h4 class="icon-head head-edit-form fieldset-legend">Thêm mới danh mục hỗ trợ</h4>
            <div class="form-buttons"></div>
        </div>
        <div class="fieldset " id="user_base_fieldset">
            <div class="hor-scroll">             
                @Html.Partial("_CreateOrEdit", Model)
            </div>
        </div>
    </div>
}
